# sequence labelling algorithm to be used, for CRF select wapiti, for deep learning select delft
grobid.software.engine=wapiti
#grobid.software.engine=delft

# additional embeddings info for DeLFT only (default is glove, could be elmo or flair)
grobid.software.delft.install=../delft
grobid.software.delft.architecture=bilstm-crf
grobid.software.delft.embeddings=glove

# entity-fishing server information for performing entity disambiguation
#grobid.software.entity-fishing.host=cloud.science-miner.com/nerd
#grobid.software.entity-fishing.port=
grobid.software.entity-fishing.host=localhost
grobid.software.entity-fishing.port=8090

# the following are normally set to the default grobid paths and should likely never been changed, except to overide 
# the default paths for a specific deployment
grobid.software.corpusPath=../software-mentions/resources/dataset/software/corpus
grobid.software.templatePath=../software-mentions/resources/dataset/software/crfpp-templates/software.template
grobid.software.tmpPath=../grobid-home/tmp/
grobid.home=../grobid-home
grobid.properties=../grobid-home/config/grobid.properties
